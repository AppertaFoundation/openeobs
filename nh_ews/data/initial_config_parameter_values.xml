<openerp>
    <data>
        <record id="initial_config_parameter_values" model="nh.clinical.frequencies.ews_settings">
            <field name="no_risk_minimum">1</field>
            <field name="no_risk_maximum">1440</field>
            <field name="no_risk">720</field>
            <field name="low_risk_minimum">1</field>
            <field name="low_risk_maximum">360</field>
            <field name="low_risk">360</field>
            <field name="medium_risk_minimum">1</field>
            <field name="medium_risk_maximum">60</field>
            <field name="medium_risk">60</field>
            <field name="high_risk_minimum">1</field>
            <field name="high_risk_maximum">10</field>
            <field name="high_risk">10</field>
            <field name="placement_minimum">1</field>
            <field name="placement_maximum">360</field>
            <field name="placement">15</field>
            <field name="obs_restart_minimum">1</field>
            <field name="obs_restart_maximum">1440</field>
            <field name="obs_restart">60</field>
            <!-- The parameters below are suffixed with '_refusal' because
                 currently they are only used to decide the new observation
                 frequency following a refusal. -->
            <field name="transfer_refusal_minimum">1</field>
            <field name="transfer_refusal_maximum">360</field>
            <field name="transfer_refusal">15</field>
            <field name="unknown_risk_refusal_minimum">1</field>
            <field name="unknown_risk_refusal_maximum">360</field>
            <field name="unknown_risk_refusal">240</field>
        </record>
        <function model="nh.clinical.frequencies.ews_settings" name="execute">
            <value eval="[ref('initial_config_parameter_values')]"/>
        </function>
    </data>
</openerp>